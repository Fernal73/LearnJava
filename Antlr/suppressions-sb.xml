<?xml version="1.0" encoding="utf-8"?>
<FindBugsFilter>
  <Match>
    <Bug code="SECCRLFLOG" />
  </Match>
  <Match>
    <Or>
    <Class name="~.*[.].*Parser" />
    <Class name="~.*[.].*Parser[$].*" />
    <Class name="~.*[.].*Lexer" />
    <Class name="~.*[.].*Lexer[$].*" />
    <Class name="~.*[.].*Listener" />
    <Class name="~.*[.].*Listener[$].*" />
    <Class name="~.*[.].*Visitor" />
    <Class name="~.*[.].*Visitor[$].*" />
  </Or>
  </Match>
</FindBugsFilter>
