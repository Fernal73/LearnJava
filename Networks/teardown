#!/usr/bin/env bash
echo "Killing DaytimeServer..."
ps -ef | grep \
  'networking.DaytimeServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing TimeServer..."
ps -ef | grep \
  'networking.TimeServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing MultithreadedDaytimeServer..."
ps -ef | grep \
  'networking.MultithreadedDaytimeServer' | 
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing PooledDaytimeServer..."
ps -ef | grep \
  'networking.PooledDaytimeServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing EchoServer..."
ps -ef | grep \
  'networking.EchoServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing RandomPort..."
ps -ef | grep \
  'networking.RandomPort' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing LoggingDaytimeServer..."
ps -ef | grep \
  'networking.LoggingDaytimeServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing SingleFileHttpServer..."
ps -ef | grep \
  'networking.SingleFileHttpServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing Redirector..."
ps -ef | grep \
  'networking.Redirector' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing JHttp..."
ps -ef | grep \
  'networking.JHttp' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing SecureOrderTaker..."
ps -ef | grep \
  'networking.SecureOrderTaker' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing MainClass..."
ps -ef | grep \
  'networking.MainClass' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing ServerEcho..."
ps -ef | grep \
  'networking.ServerEcho' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing ChargenServer..."
ps -ef | grep \
  'networking.ChargenServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
echo "Killing IntgenServer..."
ps -ef | grep \
  'networking.IntgenServer' | \
  grep -v grep | \
  awk '{print $2}' \
  | parallel -I% kill %
echo $?
