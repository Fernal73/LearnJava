<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
suppressions PUBLIC "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
"https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
<suppress-xpath
       files="KeyPairGeneratorExample.java"
       checks="MagicNumberCheck"
       query="/ENUM_DEF[./IDENT[@text='KeyPairGeneratorExample']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='main']]/SLIST/LITERAL_TRY/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/NUM_INT[@text='2048']"/>
  <suppress-xpath files="MessageDigestExample.java"
                  checks="MagicNumberCheck"
                  query="/ENUM_DEF[./IDENT[@text='MessageDigestExample']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='main']]/SLIST/LITERAL_TRY/SLIST/LITERAL_FOR/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/METHOD_CALL/ELIST/EXPR/BAND/NUM_INT[@text='0xFF']" />
</suppressions>
